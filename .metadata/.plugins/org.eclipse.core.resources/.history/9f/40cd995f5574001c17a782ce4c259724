package zprePractice2;

public class Pattern {

	public static void main(String[] args)
	{	
		int cnt=1;
		for (int i = 0; i <5; i++) 
		{
			for (int j = 0; j <5; j++) 
			{
				if(j+i>=4)
				{
					if(i>=2 && j>=0&&j<=4)
						System.out.print(cnt++ +" ");
					else
					System.out.print(1+" ");
				}
				else
					System.out.print(" ");
			}
			System.out.println();
		}
	}

}
