
public class Merge_Array2 {

	public static void main(String[] args) {
		char a[] = {'a','b','c','d','e'};
		char b[] = {'a','b','c','d','e'};
		int index=0;
		int c[]=new int[a.length+b.length];
		for (int i = 0; i < c.length; i++)
		{
			if(i<a.length)
			c[index++]=a[i];
		if(i<b.length)
			c[index++]=b[i];
		}
		for (int i = 0; i < c.length; i++) 
			System.out.println(c[i]);
	}

}
