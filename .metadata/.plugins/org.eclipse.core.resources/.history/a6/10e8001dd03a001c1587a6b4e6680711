package DoublyLinkedListNov1stEx1;

public class mainMethod {

	public static void main(String[] args) {

 myLinkedList<Integer> list=new myLinkedList<>();
 
list.add(10);
list.add(20);
list.add(30);
list.add(50);
list.add(60);
list.add(70);

System.out.println("....forword print.....");
list.forWordPrint();
System.out.println();

System.out.println("....backWord print.....");
list.backWordPrint();
System.out.println();

System.out.println("....add with Index method....");
list.addwithIndex(3, 40);
list.forWordPrint();
System.out.println();


System.out.println("....set method....");
list.set(1, 20);
list.forWordPrint();
System.out.println();

System.out.println("....get method....");
System.out.println(list.get(1));
System.out.println();

System.out.println("....contain method....");
System.out.println(list.contains(10));
System.out.println();

System.out.println("....indexOf method....");
System.out.println(list.indexof(101));
System.out.println();

System.out.println("....isEmpty method....");
list.isEmpty();
System.out.println();


System.out.println("....clear method....");
list.clear();


	}

}