package Basic;



public class Apattern35 {

	public static void main(String[] args) {
     int cnt=0;
		loopi(0,cnt);
	}

	private static void loopi(int i,int cnt) {
     
		if(i<5)
		{
			loopj(i,0);
			System.out.println();
			i++;
			loopi(i,cnt);
		}
	}

	private static void loopj(int i, int j) {
       
		if(j<5)
		{
			if(j-i<=0)
			
				System.out.print((char)(i+65));
			
			else
				System.out.print(" ");
			j++;
			loopj(i,j);
		}
	}

}

