package ArraysQuestions;

import java.util.Arrays;

public class bubbleSorting {

	public static void main(String[] args) {

int a[]= {1,3,8,6,4,9};

for (int i = 0; i < a.length; i++) 
{
	for (int j = i+1; j < a.length-1; j++)
	{
		if(a[j]>a[j+1])
		{
			int temp=a[j];
			a[j]=a[j+1];
			a[+1]=temp;
		}
	}
}
System.out.println(Arrays.toString(a));
	}

}
