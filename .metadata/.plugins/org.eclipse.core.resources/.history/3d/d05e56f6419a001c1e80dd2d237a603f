package ArrayQue;

import java.util.Iterator;

public class Boxer1 {
	public static void main(String args[]) {
		int[] a = { 4, 7, 3, 9, 2, 0 };

		int max=Integer.MIN_VALUE;
		int smax=Integer.MIN_VALUE;
		for (int i = 0; i < a.length; i++)
		{
			if(a[i]>max)
				max=a[i];
			if(a[i]<max && a[i]>smax)
				smax=a[i];
		}
		System.out.println(smax);

	}
}