
public class Merge_Array {

	public static void main(String[] args) {
		int a[] = {11,22,33,44};
		int b[] = {11,22,33,44};
		int index=0;
		int c[]=new int[a.length+b.length];
		for (int i = 0; i < a.length; i++)
		{
			c[index++]=a[i];
		}
		for (int i = 0; i < a.length; i++)
		{
			c[index++]=b[i];
		}
for (int i = 0; i < c.length; i++) 
{
	System.out.println(c[i]);
}
	}

}
