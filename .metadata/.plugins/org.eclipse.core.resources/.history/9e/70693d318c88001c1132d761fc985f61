package ERPQUE;


	public class h {

		//StartOfMainMethod
		public static void main(String[] args) 
		{
			int[][] a= {
					{5,1,3,},
					{2,4,3},
					{4,3,2}
			};
			myCode(a);
		}
		//EndOfMainMethod

		private static void myCode(int[][] a) 
		{
			//WriteCode Here
	int sum=0;
	for(int i=0;i<a.length;i++)
	{
	for(int j=0;j<a.length;j++)
	{
	if(j-i==0)
	{
		sum=sum+a[i][j];
	}
	}
	}
	System.out.println(sum);
			
		}
	}


