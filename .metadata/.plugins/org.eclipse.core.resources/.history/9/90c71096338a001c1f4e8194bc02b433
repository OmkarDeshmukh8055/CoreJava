package Sachin_DAda;

public class Q3 {

		//StartOfMainMethod
		public static void main(String[] args) 
		{
			int[] a= 
			{
				1,2,3,4,5,6,7,8
			}
			;
			myCode(a);
		}
		//EndOfMainMethod
		private static void myCode(int[] a) 
		{
			//Write code here
			int j=a[a.length-1];
			int i=0;
			while(i<j)
			{
				int temp=a[i];
				a[i]=a[j];
				a[j]=temp;
				j--;
				i++;
			}
		}
	}

