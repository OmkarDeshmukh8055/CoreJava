package Sachin_DAda;

public class test {

	public static void main(String[] args)
	{
for (int i = 0; i < 6; i++) 
{
	for (int j = 0; j <6; j++)
	{
		if(i==0 && j==0)
			System.out.print("*");
		else if(i==1 && j==4 && j==5)
			System.out.print("");
		else if(i==2 && j<=2)
			System.out.print("*");
		else if(i==3 && j>1)
			System.out.print("*");
		else if(i==4 && j<=4)
			System.out.print("*");
		else if(i==5 )
			System.out.print("*");
		else 
			System.out.print(" ");
			
	}
	System.out.println();
}
	}

}
