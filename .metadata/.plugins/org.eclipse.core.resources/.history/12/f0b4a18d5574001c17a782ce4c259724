package zprePractice2;

public class Pattern {

	public static void main(String[] args)
	{	
		for (int i = 0; i <5; i++) 
		{		int cnt=1;

			for (int j = 0; j <5; j++) 
			{
				if(j+i>=4)
				{
					if(i>=2 &&j<=2)
					{
						System.out.print(cnt++ +" ");
					}
					else
					System.out.print(1+" ");
				}
				else
					System.out.print(" ");
			}
			System.out.println();
		}
	}

}
