package array;

public class Q30 {

	public static void main(String[] args) {

		int a[] = { 1, 1, 1, 1, 1, 2, 2, 3, 4 };
		int b[] = { 1, 2, 4 };
		int cnt=0;
		for (int i = 0; i < b.length; i++) 
		{
			for (int j = 0; j < a.length; j++)
			{
				if(b[i]==a[j])
					cnt++;
				if(cnt==2)
				{
					cnt++;
					break;
				}
				
				
			}
		}
		System.out.println(cnt);
	}

}
