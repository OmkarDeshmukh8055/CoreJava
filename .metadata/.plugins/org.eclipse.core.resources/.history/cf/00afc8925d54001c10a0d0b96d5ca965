package array;

public class Q30againagain {

	public static void main(String[] args) {
		int a[]= {1,1,1,1,1,2,2,4};
		int b[]= {1,2,1,4};
		
		int cnt=0;
		for (int i = 0; i < a.length; i++)
		{	int count=0;
			for (int j = 0; j < b.length; j++)
			{
				if(a[i]==b[j])
				{
					cnt++;
				break;
				}
			}
			
		}
		System.out.println(cnt);
	}

}
