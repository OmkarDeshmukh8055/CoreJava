package Basic;



public class Apattern35 {

	public static void main(String[] args) {
     int cnt=0;
		loopi(0,cnt);
	}

	private static void loopi(int i,int cnt) {
     
		if(i<6)
		{
			loopj(i,0,cnt);
			System.out.println();
			i++;
			loopi(i,cnt+1);
		}
	}

	private static void loopj(int i, int j,int cnt) {
       
		if(j<5)
		{
			if(j-i<=0)
			
				System.out.print((char)(cnt+65));
			
			else
				System.out.print(" ");
			j++;
			cnt++;
			loopj(i,j,cnt);
		}
	}

}

