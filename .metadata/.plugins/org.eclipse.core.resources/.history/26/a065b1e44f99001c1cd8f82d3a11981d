package Basic;

public class Apattern30 {

	public static void main(String[] args) {
		printi(0);
	}

	private static void printi(int i) 
	{
		if (i < 12) 
		{
			printJ(i, 0);
			System.out.println();
			printi(i + 1);
		}
	}

	private static void printJ(int i, int j)
	{
		if (j <7) 
		{
			if (j+i<=5 ||j-i<-4)
				System.out.print((char)(6-j+65));
			else
				System.out.print(" ");
			printJ(i, j + 1);
		}
	}

}
