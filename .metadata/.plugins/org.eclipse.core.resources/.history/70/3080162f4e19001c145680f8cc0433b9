package day5;

public class Hw20 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0;i<9;i++) {
			for(int j=0;j<10;j++) {
				System.out.println();
				if(j=4&&j+i<4)
					System.out.print("* ");
				else
					System.out.print(" ");
	
}
			System.out.println();
	
	}

}
}
