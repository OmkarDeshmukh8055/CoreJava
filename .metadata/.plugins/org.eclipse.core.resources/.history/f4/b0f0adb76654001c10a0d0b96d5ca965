package array;

import java.util.Arrays;

public class selection {

	public static void main(String[] args) {
		int a[]= {1,9,2,8,3,7,4,5,6};
		
		for (int i = 0; i < a.length; i++) 
		{
			int min=i;
			for (int j = i+1; j < a.length; j++) 
			{
				if(a[j]>a[min])
				{
					min=j;
				}
			}
		}
		System.out.println(Arrays.toString(a));
	}

}
