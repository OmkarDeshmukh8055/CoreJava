package test216sep;

public class q6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a=6;
int b=5;
int c=12;
if(a>b && a>c)
	System.out.println("a is max num");
else if
	(b>a && b>c)
	System.out.println("b is max num");
else
	(c>a && c>b)
	System.out.println("c is max num");
	}

}
