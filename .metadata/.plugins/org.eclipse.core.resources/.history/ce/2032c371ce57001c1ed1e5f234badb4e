package array;

public class Leader {

	public static void main(String[] args) {

		int a[]= {1,9,2,3,4,6,4};
		
		for (int i = 0; i < a.length; i++) 
		{	int leader=0;
			for (int j = i; j < a.length; j++)
			{
				if(a[i]>a[j])
				{
					leader=a[i];
				}
				System.out.println(leader);
			}
		}
	}

}
