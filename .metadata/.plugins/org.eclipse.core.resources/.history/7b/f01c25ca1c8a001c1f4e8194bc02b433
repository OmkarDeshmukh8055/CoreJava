package Sachin_DAda;

public class Q2 {
	
		//StartOfMainMethod
		public static void main(String[] args) 
		{
			int[] a= 
			{
				1,3,5,6,2,3,4,5,97,1,23,
			}
			;
			myCode(a);
		}
		//EndOfMainMethod
		private static void myCode(int[] a) 
		{
			//WriteCode Here
			int cnt=0;
			for(int i=0 ; i<a.length;i++)
			{
				for(int j=i+1;j<a.length;j++)
				{
					if(a[i]==a[j])
					{
						cnt++;
					}
				}
			}
			System.out.println(cnt);
		}
	}


