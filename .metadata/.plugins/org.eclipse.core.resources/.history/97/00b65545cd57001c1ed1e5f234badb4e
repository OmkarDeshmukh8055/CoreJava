package array;

public class minmax {

	public static void main(String[] args) {

		int a[]= {2,3,6,11};
		int max=	Integer.MIN_VALUE;	
		int max2=	Integer.MIN_VALUE;	

		int min=Integer.MAX_VALUE;
		for (int i = 0; i < a.length; i++) 
		{
			if(a[i]>max )
			{
				max=a[i];
			}
			if(a[i]!=max && max2<a[i] )
			{
				max2=a[i];
			}
				
		}

		System.out.println(max);
		System.out.println(max2);

	}

}
