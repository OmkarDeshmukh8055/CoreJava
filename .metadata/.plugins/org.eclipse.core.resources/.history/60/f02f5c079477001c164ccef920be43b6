package mix;

public class Q23Array {

	public static void main(String[] args) {
		int a[][]= {{1,2,3},
				   {2,9,4},
				   {3,4,8}};
		for (int i = 0; i < a.length; i++)
		{
			for (int j = 0; j < a.length; j++)
			{
				if(i==0 || j==0 || i==a.length-1 ||j==a.length-1)
					System.out.print(a[i][j]);
				else
					System.out.print(" ");
			}
			System.out.println();
		}
		}

}
